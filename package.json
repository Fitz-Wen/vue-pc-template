{
  "name": "vue-video",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "babel-plugin-import": "^1.11.0",
    "core-js": "^2.6.5",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "vue": "^2.6.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "ant-design-vue": "^1.3.7",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^8.1.4",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "curly": 2,
      "guard-for-in": 2,
      "indent": [
        2,
        2
      ],
      "no-debugger": 2,
      "no-empty": 2,
      "no-eval": 2,
      "no-unused-expressions": 1,
      "prefer-const": 1,
      "radix": 1,
      "linebreak-style": [
        0,
        "windows"
      ],
      "no-cond-assign": 2,
      "no-unreachable": 1,
      "eqeqeq": [
        2,
        "smart"
      ],
      "no-redeclare": 2,
      "no-undef": 2,
      "block-spacing": 2,
      "key-spacing": [
        2,
        {
          "afterColon": true
        }
      ],
      "no-duplicate-imports": 2,
      "no-duplicate-case": 2,
      "no-multi-spaces": 2,
      "no-dupe-args": 2,
      "no-dupe-keys": 2,
      "no-console": 0,
      "no-var": 2,
      "no-unused-vars": 2,
      "no-const-assign": 2,
      "no-lone-blocks": 2,
      "quotes": [
        2,
        "single"
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
